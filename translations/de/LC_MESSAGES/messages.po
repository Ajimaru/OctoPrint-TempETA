msgid "Browser notifications"
msgstr "Browser-Benachrichtigungen"

#: octoprint_temp_eta/templates/temp_eta_settings.jinja2:629
msgid "Calculates heating rate (°C/second)"
msgstr "Berechnet die Heizrate (°C/Sekunde)"

#: octoprint_temp_eta/templates/temp_eta_settings.jinja2:62
msgid "Calculation Algorithm"
msgstr "Berechnungsalgorithmus"

#: octoprint_temp_eta/templates/temp_eta_settings.jinja2:52
#: octoprint_temp_eta/templates/temp_eta_settings.jinja2:157
msgid "Celsius (°C)"
msgstr "Celsius (°C)"

#: octoprint_temp_eta/templates/temp_eta_settings.jinja2:158
msgid "Celsius + Fahrenheit (°C/°F)"
msgstr "Celsius + Fahrenheit (°C/°F)"

#: octoprint_temp_eta/templates/temp_eta_settings.jinja2:275
msgid "Choose which events should play a sound alert."
msgstr "Wähle, welche Ereignisse einen Sound abspielen sollen."

#: octoprint_temp_eta/templates/temp_eta_settings.jinja2:336
msgid "Choose which events should show a browser notification."
msgstr "Wähle, welche Ereignisse eine Browser-Benachrichtigung anzeigen sollen."

#: octoprint_temp_eta/templates/temp_eta_settings.jinja2:170
msgid "Color mode"
msgstr "Farbmodus"

#: octoprint_temp_eta/templates/temp_eta_tab.jinja2:66
msgid "Configure algorithm and threshold in Settings → Temperature ETA"
msgstr ""
"Algorithmus und Schwellwert unter Einstellungen → Temperatur-ETA "
"konfigurieren"

#: octoprint_temp_eta/templates/temp_eta_settings.jinja2:580
#: octoprint_temp_eta/templates/temp_eta_settings.jinja2:603
msgid "Confirm reset"
msgstr "Zurücksetzen bestätigen"

#: octoprint_temp_eta/templates/temp_eta_settings.jinja2:16
msgid "Cool-down ETA"
msgstr "Cool-Down-ETA"

#: octoprint_temp_eta/templates/temp_eta_settings.jinja2:437
msgid "Cool-down fit window (seconds)"
msgstr "Cool-Down-Fit-Fenster (Sekunden)"

#: octoprint_temp_eta/templates/temp_eta_settings.jinja2:427
msgid "Cool-down hysteresis (°C)"
msgstr "Cool-Down-Hysterese (°C)"

#: octoprint_temp_eta/templates/temp_eta_settings.jinja2:375
msgid "Cool-down mode"
msgstr "Cool-Down-Modus"

#: octoprint_temp_eta/templates/temp_eta_settings.jinja2:397
msgid "Cool-down target (bed)"
msgstr "Cool-Down-Ziel (Bett)"

#: octoprint_temp_eta/templates/temp_eta_settings.jinja2:407
msgid "Cool-down target (chamber)"
msgstr "Cool-Down-Ziel (Chamber)"

#: octoprint_temp_eta/templates/temp_eta_settings.jinja2:387
msgid "Cool-down target (tool0)"
msgstr "Cool-Down-Ziel (Tool0)"

